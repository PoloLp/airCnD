<div class="container-desk-form">
  <%= simple_form_for (@desk) do |f| %>
    <%= f.label :Desk_title %>
    <%= f.input_field :title, class: "form-control" %>
    <%= f.label :Desk_address %>
    <%= f.input_field :address, class: "form-control" %>
    <%= f.label :Desk_description %>
    <%= f.input_field :description, class: "form-control" %>
    <%= f.label :Desk_price %>
    <%= f.input_field :price, class: "form-control" %>
    <%= f.label :Desk_photo %>
    <%= f.input_field :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>
</div>
