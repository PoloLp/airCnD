<div class="container">
  <h1 class= "text-center"><%= @desk.title %></h1>
  <div class="img img-show">
    <%=  cl_image_tag(@booking.desk.photo, :width=>1000, :height=>500, :class=> " img img-show card-margin", :crop=>"fill") %>
    <div class="left-top">
      <p>Booked the <%= @booking.start_at.strftime('%Y-%m-%d') %> at <%= @booking.start_at.strftime('%H:%M') %></p>
      <p> till <%= @booking.end_at.strftime('%Y-%m-%d') %> at <%= @booking.end_at.strftime('%H:%M') %></p>
    </div>
    <h2 class="right-bottom">Booked</h2>
  </div>
  <div class="booking-time">
    <p>Booked the <%= @booking.start_at.strftime('%Y-%m-%d') %> at <%= @booking.start_at.strftime('%H:%M') %></p>
    <p> till <%= @booking.end_at.strftime('%Y-%m-%d') %> at <%= @booking.end_at.strftime('%H:%M') %></p>
  </div>
  <div>
    <%= render 'bookings/add_review' %>
  </div>
</div>


